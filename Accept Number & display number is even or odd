/*Accept Number & display number is even or odd */
class EvenOdd
{
  public static void main(String arg[])
   {
     int n=5;
     if(n%2==0)
       System.out.println("Number is Even");
     else
       System.out.println("Number is Odd");
   }
}    

/*
Output:-
bcs@PRATHAMA:~$ javac evenodd.java
bcs@PRATHAMA:~$ java EvenOdd
Number is Odd
*/
