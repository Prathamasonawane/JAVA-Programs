/* Accept string and display substring */
/* substring(startindex,endindex);
   substring(startindex);
*/
import java.util.*;
class Demo
{
  public static void main(String arg[])
  {
    String s1="Shrirampur";
     //String s2=s1.substring(4,6);
    String s2=s1.substring(4,7);
    System.out.println(s2);
  }
}
/*
Output:-
bcs@PRATHAMA:~$ javac p7.java
bcs@PRATHAMA:~$ java Demo
ram
*/
